[alias]
	co = checkout
	ci = commit
	cm = commit -m 
	cf = commit -n -m
	recm = commit --amend --no-edit
	st = status
	br = branch
	dc = diff --cached
	rv = remote -v
	pp = push
	hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=relative
	type = cat-file -t
	dump = cat-file -p
	last = log -1 HEAD
	unstage = reset HEAD --
	track = rev-parse --symbolic-full-name @{u}
	brs = branch -vv
[push]
	default = simple
[pull]
	rebase = true
[merge]
[core]
	excludesfile = /home/jj/.gitignore_global
[filter "cat"]
	clean = test_append
